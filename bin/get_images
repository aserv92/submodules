#!/usr/bin/env bash

set -eu

export DOCKER_REGISTRY_HOST=registry.hub.docker.com
export DOCKER_REGISTRY_ACCOUNT=medology
export GCR_HOST=gcr.io
export GCR_US_HOST=us.gcr.io
export GOOGLE_PROJECT_ID=docker-1000101

export PHP_CS_FIXER_ACCOUNT=cytopia
export PHP_CS_FIXER_REPO=php-cs-fixer
export PHP_CS_FIXER_TAG=latest-php7.4-0.3
export PHP_CS_FIXER_IMAGE=$DOCKER_REGISTRY_HOST/$PHP_CS_FIXER_ACCOUNT/$PHP_CS_FIXER_REPO:$PHP_CS_FIXER_TAG

export GHERKIN_CS_FIXER_REPO=gherkin-cs-fixer
export GHERKIN_CS_FIXER_TAG=04-23-2020
export GHERKIN_CS_FIXER_IMAGE=$DOCKER_REGISTRY_HOST/$DOCKER_REGISTRY_ACCOUNT/$GHERKIN_CS_FIXER_REPO:$GHERKIN_CS_FIXER_TAG

export GCLOUD_SDK_ACCOUNT=chekote
export GCLOUD_SDK_REPO=google-cloud-sdk
export GCLOUD_SDK_TAG=334.0.0
export GCLOUD_SDK_IMAGE=$DOCKER_REGISTRY_HOST/$GCLOUD_SDK_ACCOUNT/$GCLOUD_SDK_REPO:$GCLOUD_SDK_TAG

export PRETTIER_ESLINT_REPO=prettier-eslint
export PRETTIER_ESLINT_TAG=2020-07-14
export PRETTIER_ESLINT_IMAGE=$DOCKER_REGISTRY_HOST/$DOCKER_REGISTRY_ACCOUNT/$PRETTIER_ESLINT_REPO:$PRETTIER_ESLINT_TAG

export BATS_REPO=bats
export BATS_TAG=php7.3-laravel5.7
export BATS_IMAGE=$GCR_US_HOST/$GOOGLE_PROJECT_ID/$BATS_REPO:$BATS_TAG

export JAVA_STYLER_REPO=java-cs-fixer
export JAVA_STYLER_TAG=jdk15-1.9-9ee0482
export JAVA_STYLER_IMAGE=$GCR_HOST/$GOOGLE_PROJECT_ID/$JAVA_STYLER_REPO:$JAVA_STYLER_TAG

export SED_REPO=alpine
export SED_TAG=3.8
